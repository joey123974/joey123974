// 定义 LED 引脚
const int led1 = 8;
const int led2 = 9;

// 初始化
void setup() {
  pinMode(led1, OUTPUT);
  pinMode(led2, OUTPUT);
}

void loop() {
  // 点亮 LED1，延迟 0.5 秒（500 毫秒）
  digitalWrite(led1, HIGH);
  delay(500);
  digitalWrite(led1, LOW); // 关闭 LED1

  // 点亮 LED2，延迟 0.5 秒（500 毫秒）
  digitalWrite(led2, HIGH);
  delay(500);
  digitalWrite(led2, LOW); // 关闭 LED2
}
